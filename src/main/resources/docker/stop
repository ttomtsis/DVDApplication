# Disconnect from network
docker network disconnect dvd_network dvd_mysql_server
docker network disconnect dvd_network dvd_spring_server

# Delete network
docker network rm dvd_network

# Stop containers
docker stop dvd_spring_server 
docker stop dvd_mysql_server

# Delete containers
docker rm dvd_spring_Server
docker rm dvd_mysql_server

# Delete images
docker rmi dvd_spring_server:latest
docker rmi dvd_mysql_server:latest

# Delete volume
docker volume rm mydb_volume